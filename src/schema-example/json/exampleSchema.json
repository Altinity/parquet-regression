{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title:": "JSON Schema for Altinity Parquet Regression",
  "options": {
    "writerVersion": "1.0",
    "compression": "snappy",
    "rowGroupSize": "default",
    "pageSize": "default",
    "encodings": ["DICTIONARY", "BYTE_STREAM_SPLIT"],
    "bloomFilter": "all"
  },
  "schema": [
    {
      "name": "age",
      "type": "uint8"
    },
    {
      "name": "name",
      "type": "string"
    },
    {
      "name": "city",
      "type": "string"
    },
    {
      "name": "state",
      "type": "int64"
    }
  ]
}